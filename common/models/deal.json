{
  "name": "deal",
  "base": "PersistedModel",
  "options": {
    "mongodb": {
      "collection": "deals"
    }
  },
  "properties": {
    "shopID": {
      "type": "string",
      "required": true
    },
    "merchantID": {
      "type": "string",
      "required": true
    },
    "seller": {
      "type": "object",
      "required": true
    },
    "fee": {
      "type": "number",
      "required": true
    },
    "quantity": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "merchant": {
      "type": "belongsTo",
      "model": "merchant",
      "foreignKey": "merchantID"
    },
    "shop": {
      "type": "belongsTo",
      "model": "shop",
      "foreignKey": "shopID"
    },
    "bill": {
      "type": "belongsTo",
      "model": "bill",
      "foreignKey": "billID"
    }
  },
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": []
}
